[Unit]
Description=Qapish AI Colocation Platform Network
Documentation=man:podman-systemd.unit(5)

[Network]
NetworkName=qapish-net
Driver=bridge
Internal=false
IPv6=false

# Subnet configuration
Subnet=10.89.0.0/24
Gateway=10.89.0.1

# DNS settings
DNS=1.1.1.1
DNS=8.8.8.8

# Labels for identification
Label=app=qapish
Label=environment=production

[Install]
WantedBy=multi-user.target default.target
