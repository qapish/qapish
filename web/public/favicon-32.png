<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <style>
      .bg { fill: #0F172A; }
      .accent { fill: #06B6D4; }
      .text {
        fill: #E2E8F0;
        font-family: 'Inter', sans-serif;
        font-weight: 900;
        font-size: 20px;
        text-anchor: middle;
        dominant-baseline: central;
      }
      @media (prefers-color-scheme: dark) {
        .bg { fill: #E2E8F0; }
        .text { fill: #0F172A; }
      }
    </style>
  </defs>

  <!-- Background circle -->
  <circle cx="16" cy="16" r="16" class="bg"/>

  <!-- Q letter -->
  <text x="16" y="16" class="text">Q</text>

  <!-- Small accent dot -->
  <circle cx="20" cy="20" r="2" class="accent"/>
</svg>
